{
    "repository": "git@github.com:openmdao/dymos",

    "triggers": [
        "git@github.com:openmdao/OpenMDAO",
        "git@github.com:openmdao/MBI"
    ],

    "preinstall": [
        "unset USE_PROC_FILES"
    ],

    "conda": [
        "python=3.10",
        "gfortran=12",
        "numpy",
        "scipy",
        "jupyter-book"
    ],

    "conda-forge": [
        "mpi4py",
        "petsc4py"
    ],

    "dependencies": [
        "sqlitedict"
    ],

    "extras": "[all]",

    "postinstall": [
        "pip install git+https://github.com/OpenMDAO/build_pyoptsparse",
        "build_pyoptsparse -s ~/dev/snopt7/src"
    ],

    "notify": [
        "!subteam^S01PJ2C9GPN"
    ]
}
